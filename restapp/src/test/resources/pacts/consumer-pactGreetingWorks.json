{
  "provider": {
    "name": "pactGreetingWorks"
  },
  "consumer": {
    "name": "consumer"
  },
  "interactions": [
    {
      "description": "A request to greeting/Ivan",
      "request": {
        "method": "GET",
        "path": "/greeting/Ivan"
      },
